{
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "afterDelay",
  "git.enableSmartCommit": true,
  "editor.minimap.enabled": false,
  "git.autofetch": true,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.gpuAcceleration": "on",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "redhat.telemetry.enabled": true,
  "git.confirmSync": false,
  "hediet.vscode-drawio.resizeImages": null,
  "markdown-preview-enhanced.previewTheme": "monokai.css",
  "markdown-preview-enhanced.revealjsTheme": "moon.css",
  "markdown-preview-enhanced.enablePreviewZenMode": true,
  "markdown-preview-enhanced.pandocArguments": [],
  "markdown-preview-enhanced.usePandocParser": true,
  "markdown.extension.completion.enabled": true,
  "markdown.extension.tableFormatter.enabled": false,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.extension.toc.orderedList": true,
  "markdown-preview-enhanced.codeBlockTheme": "monokai.css",
  "markdown-mermaid.lightModeTheme": "base",
  "markdown-mermaid.darkModeTheme": "forest",
  "markdown-preview-enhanced.mermaidTheme": "dark",
  "github.copilot.enable": {
    "markdown": "false"
  },
  "explorer.confirmDelete": false,
  "foam.files.ignore": [],
  "githubPullRequests.createOnPublishBranch": "never",
  "diffEditor.renderSideBySide": true,
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  "docker.extension.enableComposeLanguageServer": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "diffEditor.experimental.showMoves": true,
  "window.confirmSaveUntitledWorkspace": false,
  "editor.lineNumbers": "on",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": [],
        "commands": [
            {
                "command": "workbench.action.toggleEditorLineNumbers",
                "args": "relative"
            }
        ]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
        "before": [],
        "commands": [
            {
                "command": "workbench.action.toggleEditorLineNumbers",
                "args": "on"
            }
        ]
    }
  ],
  "files.associations": {
    "*.json": "plaintext"
  },
  "editor.fontFamily": "'Meslo LG M Nerd Font', 'FiraCode Nerd Font', monospace",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "'Meslo LG M Nerd Font', 'FiraCode Nerd Font', monospace"
}
